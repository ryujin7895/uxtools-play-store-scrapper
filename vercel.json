{
  "framework": "remix",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "routes": [
    {
      "src": "/build/(.+)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ]
}